#include <stdint.h>
#include <stm32f407xx.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

void SoftwareDelay(uint32_t tick)
{
	for (uint32_t i = 0; i <= tick; i++);
}

int main(void)
{
	GPIO_Handle_t LD4;

	LD4.pGPIOx = GPIOD;

	GPIO_Config(&LD4, GPIO_PIN_NO_12, GPIO_MODE_OUT, GPIO_SPEED_FAST, GPIO_OP_TYPE_PP, GPIO_NO_PUPD, 0);
	LD4.GPIO_PinConfig.GPIO_PinNumber = GPIO_PIN_NO_12;
	LD4.GPIO_PinConfig.GPIO_PinMode = GPIO_MODE_OUT;
	LD4.GPIO_PinConfig.GPIO_PinSpeed = GPIO_SPEED_FAST;
	LD4.GPIO_PinConfig.GPIO_PinOPType = GPIO_OP_TYPE_PP;
	LD4.GPIO_PinConfig.GPIO_PinPuPdControl = GPIO_NO_PUPD;

	GPIO_PeriClockControl(GPIOD, ENABLE);
	GPIO_Init(&LD4);

    /* Loop forever */
	while(1)
	{
		GPIO_ToggleOutputPin(GPIOD, GPIO_PIN_NO_12);
		SoftwareDelay(500000);
	}
}
